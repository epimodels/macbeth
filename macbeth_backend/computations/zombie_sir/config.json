{
    "Title": "ZombieSIR",
    "Version": 1.0,
    "Author": "Eric Lofgren (adapted by Kyle Hurd)",
    "Description": "Susceptible-Infected-Recovered (SIR) of a population of zombies.",
    "Type": "Deterministic",
    "Authorlink": "https://docs.idmod.org/projects/emod-generic/en/latest/model-sir.html",
    "Parameters": [
        {
            "Name": "Infect Probability",
            "VariableName": "infect_prob",
            "Type": "float",
            "Description": "The probability that a susceptible person will become infected.",
            "Min": 0.0,
            "Max": 100.0,
            "DefaultValue": 0.5
        },
        {
            "Name": "Infect Duration",
            "VariableName": "infect_duration",
            "Type": "double",
            "Description": "The duration of the infection.",
            "Min": -1.0,
            "Max": 1,
            "DefaultValue": 0.1
        }
    ],
    "GraphingData": {
        "X": {
            "Name": "Time",
            "VariableName": "t",
            "Type": "float",
            "Description": "Time in hours.",
            "Units": "hours"
        },
        "Y": [
            {
                "Name": "Susceptible",
                "VariableName": "s",
                "Type": "float",
                "Description": "Number of susceptible individuals.",
                "Units": "individuals"
            },
            {
                "Name": "Infected",
                "VariableName": "i",
                "Type": "float",
                "Description": "Number of infected individuals.",
                "Units": "individuals"
            },
            {
                "Name": "Recovered",
                "VariableName": "r",
                "Type": "float",
                "Description": "Number of recovered individuals.",
                "Units": "individuals"
            }
        ]
    }
}
